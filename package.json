{
  "name": "@theace0296/steamworks",
  "author": "TheAce0296",
  "license": "MIT",
  "version": "1.1.2",
  "description": "A wrapper library to call the Steamworks API from nodeJS.",
  "keywords": [
    "steam",
    "steamworks"
  ],
  "homepage": "https://github.com/theace0296/steamworks-node/wiki",
  "bugs": {
    "url": "https://github.com/theace0296/steamworks-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theace0296/steamworks-node"
  },
  "main": "index.js",
  "bin": {
    "steamworks-init": "./utilities/init.js"
  },
  "files": [
    "lib",
    "swig-binaries",
    "utilities",
    "index.d.ts",
    "index.js",
    "README.md",
    "CMakeLists.txt"
  ],
  "scripts": {
    "test": "node ./index.test.js",
    "build": "npm-run-all preinstall-steam swig cmake generate-types",
    "build:debug": "npm-run-all preinstall-steam swig cmake:debug generate-types",
    "preinstall-steam": "node ./utilities/preinstall-steam.js",
    "swig": "run-script-os",
    "swig:default": "export SWIG_LIB=$(node -p 'process.cwd()')/swig/Lib && ./swig/swig -Isteam -javascript -node -c++ -cpperraswarn -ignoremissing -includeall ./lib/steam_api.i",
    "swig:win32": ".\\swig\\swig.exe -Isteam -javascript -node -c++ -cpperraswarn .\\lib\\steam_api.i",
    "cmake": "npx cmake-js rebuild",
    "cmake:debug": "npx cmake-js rebuild --debug",
    "generate-types": "node ./utilities/generate_typings.js",
    "generate-docs": "npm-run-all typedoc cleanup-docs",
    "typedoc": "npx typedoc --options typedoc.json",
    "cleanup-docs": "node ./utilities/cleanup-docs.js"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "eslint": "^8.14.0",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typedoc-plugin-rename-defaults": "^0.5.1"
  },
  "dependencies": {
    "cmake-js": "^6.3.2",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "fs-extra": "^10.1.0",
    "node-bignumber": "^1.2.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "prompt-sync": "^4.2.0",
    "run-script-os": "^1.1.6"
  }
}
